    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Dist/main.js",
            "runtimeExecutable": "${workspaceFolder}/node_modules/electron/dist/electron.exe",
            "cwd": "${workspaceFolder}/Dist",
            "runtimeArgs": [
                "--nolazy"
            ],
            "stopOnEntry": false
        }
    ]
}